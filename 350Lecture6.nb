       RowBox[{
        RowBox[{"While", "[", 
         RowBox[{
          RowBox[{"Or", "[", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{
             RowBox[{"GCD", "[", 
              RowBox[{"k", ",", 
               RowBox[{"y", "[", 
                RowBox[{"n", "-", "1"}], "]"}]}], "]"}], "!=", "1"}], ",", 
            "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"GCD", "[", 
              RowBox[{"k", ",", 
               RowBox[{"y", "[", 
                RowBox[{"n", "-", "2"}], "]"}]}], "]"}], "==", "1"}], ",", 
            "\[IndentingNewLine]", 
            RowBox[{"MemberQ", "[", 
             RowBox[{
              RowBox[{"y", "/@", 
               RowBox[{"Range", "[", 
                RowBox[{"n", "-", "1"}], "]"}]}], ",", "k"}], "]"}]}], "]"}], 
          ",", "\[IndentingNewLine]", 
          RowBox[{"k", "+=", "1"}]}], "]"}], ";", "\[IndentingNewLine]", 
