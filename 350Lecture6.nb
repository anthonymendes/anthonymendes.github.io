Cell[TextData[{
 "The Legendre polynomials (see \
https://en.wikipedia.org/wiki/Legendre_polynomials ) are defined by ",
 StyleBox["p[x,0]=1, p[x,1]=x, ", "Input"],
 "and ",
 StyleBox["p[x,n]", "Input"],
 " is equal to \n\n",
 StyleBox["((2n-1)x p[x,n-1]-(n-1)p[x,n-2])/n", "Input"],
 " \n\notherwise.  Find the ",
 StyleBox["20th", "Input"],
 " Legendre polynomial, and ",
 StyleBox["Simplify", "Input"],
 " the result.  "
}], "Text",
 CellChangeTimes->{{3.922204231956718*^9, 3.9222044845738487`*^9}, {
  3.9222046195248957`*^9, 3.92220463216476*^9}, {3.92220477729163*^9, 
  3.922204786763445*^9}, {3.922204873432526*^9, 
  3.9222049268137407`*^9}},ExpressionUUID->"d5f0cf82-5f9a-4848-a46d-\
3da280ae32dd"],

Cell[BoxData[
 RowBox[{
  RowBox[{"p", "[", 
   RowBox[{"x_", ",", "n_"}], "]"}], ":=", 
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{"n", "<=", "1"}], ",", 
    RowBox[{"x", "^", "n"}], ",", 
    StyleBox[
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"2", "n"}], "-", "1"}], ")"}], "x", " ", 
         RowBox[{"p", "[", 
          RowBox[{"x", ",", 
           RowBox[{"n", "-", "1"}]}], "]"}]}], "-", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"n", "-", "1"}], ")"}], 
         RowBox[{"p", "[", 
          RowBox[{"x", ",", 
           RowBox[{"n", "-", "2"}]}], "]"}]}]}], ")"}], "/", "n"}], 
     "Input"]}], 
   StyleBox["]", "Input"]}]}]], "Input",
 CellChangeTimes->{{3.922204520990443*^9, 3.9222045515509567`*^9}, {
