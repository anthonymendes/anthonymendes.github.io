Cell[TextData[{
 "The Pell numbers are defined by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["P", "0"], "=", "0"}], ",", " ", 
    RowBox[{
     SubscriptBox["P", "1"], "=", " ", "1"}]}], TraditionalForm]],
  ExpressionUUID->"569985db-2dbb-46b8-8a5e-df1dd94df69a"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["P", "n"], "=", " ", 
    RowBox[{
     RowBox[{"2", 
      SubscriptBox["P", 
       RowBox[{"n", "-", "1"}]]}], "+", 
     SubscriptBox["P", 
