   RowBox[{"a_", ",", "b_"}], "]"}], ":=", 
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{"b", "==", "0"}], ",", "a", ",", 
    RowBox[{
     RowBox[{"Print", "[", 
      RowBox[{"{", 
       RowBox[{"a", ",", "b"}], "}"}], "]"}], ";", 
     RowBox[{"Euclidean", "[", 
      RowBox[{"b", ",", 
       RowBox[{"Mod", "[", 
        RowBox[{"a", ",", "b"}], "]"}]}], "]"}]}]}], "]"}]}]], "Input",
