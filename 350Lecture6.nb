 RowBox[{"Clear", "[", 
  RowBox[{"x", ",", "y"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"x", "[", "n_", "]"}], ":=", 
  RowBox[{
   RowBox[{"x", "[", "n", "]"}], "=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"n", "==", "0"}], ",", "1", ",", 
     RowBox[{"1", "-", 
      RowBox[{"1.4", " ", 
       RowBox[{
        RowBox[{"x", "[", 
         RowBox[{"n", "-", "1"}], "]"}], "^", "2"}]}], " ", "+", " ", 
      RowBox[{"y", "[", 
       RowBox[{"n", "-", "1"}], "]"}]}]}], "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"y", "[", "n_", "]"}], ":=", 
  RowBox[{
   RowBox[{"y", "[", "n", "]"}], "=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"n", "==", "0"}], ",", "1", ",", 
     RowBox[{".3", " ", 
      RowBox[{"x", "[", 
       RowBox[{"n", "-", "1"}], "]"}]}]}], "]"}]}]}]}], "Input",
