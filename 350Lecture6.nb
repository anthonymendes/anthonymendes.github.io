 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["P", "n"], "=", " ", 
    RowBox[{
     RowBox[{"2", 
      SubscriptBox["P", 
       RowBox[{"n", "-", "1"}]]}], "+", 
     SubscriptBox["P", 
      RowBox[{"n", "-", "2"}]]}]}], TraditionalForm]],ExpressionUUID->
  "4aee5d2a-02a5-4874-844f-c2d5e4a345de"],
