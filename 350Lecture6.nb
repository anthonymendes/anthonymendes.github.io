   RowBox[{"Which", "[", 
    RowBox[{
     RowBox[{"n", "==", "0"}], ",", "0", ",", "\[IndentingNewLine]", 
     RowBox[{"And", "[", 
      RowBox[{
       RowBox[{"Not", "@", 
        RowBox[{"MemberQ", "[", 
         RowBox[{
          RowBox[{"r", "/@", 
           RowBox[{"Range", "[", 
            RowBox[{"n", "-", "1"}], "]"}]}], ",", 
          RowBox[{
           RowBox[{"r", "[", 
            RowBox[{"n", "-", "1"}], "]"}], "-", "n"}]}], "]"}]}], ",", 
