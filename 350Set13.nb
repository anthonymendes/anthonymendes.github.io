Cell[BoxData[
 RowBox[{
  RowBox[{"Motzkin", "[", "n_", "]"}], ":=", 
  RowBox[{
   RowBox[{"Motzkin", "[", "n", "]"}], "=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"n", "==", "0"}], ",", 
     RowBox[{"{", 
      RowBox[{"{", "}"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"Join", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"Prepend", "[", 
          RowBox[{"#", ",", "0"}], "]"}], "&"}], "/@", 
        RowBox[{"Motzkin", "[", 
         RowBox[{"n", "-", "1"}], "]"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"Flatten", "[", 
        RowBox[{
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"Flatten", "@", 
            RowBox[{"{", 
             RowBox[{"1", ",", "a", ",", 
              RowBox[{"-", "1"}], ",", "b"}], "}"}]}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "0", ",", 
             RowBox[{"n", "-", "2"}]}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"a", ",", 
             RowBox[{"Motzkin", "[", "i", "]"}]}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"b", ",", 
             RowBox[{"Motzkin", "[", 
              RowBox[{"n", "-", "i", "-", "2"}], "]"}]}], "}"}]}], "]"}], ",",
          "2"}], "]"}]}], "]"}]}], "]"}]}]}]], "Input",ExpressionUUID->\
"40952842-4d72-4cbc-b200-c0bf32598702"],

Cell[TextData[{
 "a. Modify this code to define a function named ",
 StyleBox["Cata[n]", "Input"],
 " that outputs all lists of length ",
 StyleBox["n", "Input"],
 " that contain exactly ",
 StyleBox["n/2", "Input"],
 " occurrences of ",
 StyleBox["1", "Input"],
 "\[CloseCurlyQuote]s and ",
 StyleBox["n/2", "Input"],
 " occurrences of ",
 StyleBox["-1", "Input"],
 "\[CloseCurlyQuote]s such that every partial sum is nonnegative (this means \
