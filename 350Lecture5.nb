     RowBox[{
      RowBox[{"{", 
       RowBox[{
       "___", ",", "a_", ",", "___", ",", "b_", ",", "___", ",", "c_", ",", 
        "___"}], "}"}], "/;", 
      RowBox[{"a", "<", "c", "<", "b"}]}]}], "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"n", ",", "9"}], "}"}]}], "]"}]], "Input",
