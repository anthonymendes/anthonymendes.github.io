Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"SpellingBeeQ", "[", 
    RowBox[{"word_", ",", "gold_", ",", "letters_"}], "]"}], ":=", 
   RowBox[{"And", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"StringLength", "@", "word"}], ">", "3"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"StringContainsQ", "[", 
      RowBox[{"word", ",", "gold"}], "]"}], ",", "\[IndentingNewLine]", 
     RowBox[{"StringMatchQ", "[", 
      RowBox[{"word", ",", 
       RowBox[{"RegularExpression", "[", 
        RowBox[{"StringJoin", "[", 
         RowBox[{
         "\"\<[\>\"", ",", "gold", ",", "letters", ",", "\"\<]*\>\""}], "]"}],
         "]"}]}], "]"}]}], "]"}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"SpellingBee", "[", 
   RowBox[{"gold_", ",", "letters_"}], "]"}], ":=", 
  RowBox[{"Select", "[", 
   RowBox[{
    RowBox[{"WordList", "[", "]"}], ",", 
    RowBox[{
     RowBox[{"SpellingBeeQ", "[", 
      RowBox[{"#", ",", "gold", ",", "letters"}], "]"}], "&"}]}], 
   "]"}]}]}], "Input",
