Cell[BoxData[{
 RowBox[{"Clear", "[", 
  RowBox[{"f", ",", "x", ",", "y", ",", "z"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"f", "[", "t_", "]"}], ":=", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"x", "[", "t", "]"}], ",", 
    RowBox[{"y", "[", "t", "]"}], ",", 
    RowBox[{"z", "[", "t", "]"}]}], "}"}]}]}], "Input",
 CellChangeTimes->{{3.923506449315133*^9, 3.92350645107478*^9}, {
  3.9235065233584423`*^9, 3.923506525883443*^9}, {3.923506638407606*^9, 
  3.923506644279339*^9}},ExpressionUUID->"99e08eef-67ea-4563-8447-\
6521e4292bdf"],

Cell[TextData[{
 "Use this arbitrary curve to show that the acceleration of ",
 StyleBox["f", "Input"],
 " (this is the second derivative of ",
 StyleBox["f", "Input"],
 ") is orthogonal (meaning the dot product is ",
 StyleBox["0", "Input"],
 ") with the unit binomial vector ",
 StyleBox["B", "Input"],
 " (the last component of the last component in ",
 StyleBox["FrenetSerretSystem[f[t],t]", "Input"],
 ")."
}], "Text",
 CellChangeTimes->{{3.923506893568994*^9, 3.923506926782283*^9}, {
