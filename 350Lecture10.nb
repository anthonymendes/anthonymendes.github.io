Cell[TextData[{
 "Show that the function ",
 StyleBox["u[t,x]", "Input"],
 " defined in the next cell satisfies the partial differential equation\n",
 StyleBox["D[u[t,x],{t,2}] == k^2 D[u[t,x],{x,2}]", "Input"],
 " together with the initial conditions ",
 StyleBox["u[0,x] ", "Input"],
 "giving ",
 StyleBox["f[x]", "Input"],
 " and ",
 StyleBox["D[u[t,x],t]", "Input"],
 " when evaluated at ",
 StyleBox["t=0", "Input"],
 " gives ",
 StyleBox["g[x]", "Input"],
 "."
}], "Text",
 CellChangeTimes->{{3.923514755735677*^9, 3.92351493596135*^9}, {
  3.923515341517717*^9, 3.923515380646379*^9}, {3.923515422780376*^9, 
  3.923515423423966*^9}},ExpressionUUID->"e321470a-0049-4391-b6f1-\
54d2326f2c32"],

Cell[BoxData[{
 RowBox[{"Clear", "[", 
  RowBox[{"f", ",", "g", ",", "u", ",", "k"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"u", "[", 
   RowBox[{"t_", ",", "x_"}], "]"}], ":=", " ", 
  RowBox[{
   RowBox[{
    FractionBox["1", "2"], " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"f", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"-", "k"}], " ", "t"}], "+", "x"}], "]"}], "+", 
      RowBox[{"f", "[", 
       RowBox[{
        RowBox[{"k", " ", "t"}], "+", "x"}], "]"}]}], ")"}]}], "+", 
   RowBox[{
    FractionBox["1", 
     RowBox[{"2", " ", "k"}]], 
    RowBox[{
     SubsuperscriptBox["\[Integral]", 
      RowBox[{
       RowBox[{
        RowBox[{"-", "k"}], " ", "t"}], "+", "x"}], 
      RowBox[{
       RowBox[{"k", " ", "t"}], "+", "x"}]], 
     RowBox[{
      RowBox[{"g", "[", "s", "]"}], 
      RowBox[{"\[DifferentialD]", "s"}]}]}]}]}]}]}], "Input",
