Cell[TextData[{
 "Let ",
 StyleBox["f[t]", "Input"],
 " be the curve in R^3 described by the parametric equations in the cell below"
}], "Text",
 CellChangeTimes->{{3.923500292751124*^9, 3.923500327365655*^9}, {
  3.923500363188406*^9, 
  3.923500365316677*^9}},ExpressionUUID->"68be299f-1c58-4ff9-a433-\
9801ac63ded9"],

Cell[BoxData[
 RowBox[{
  RowBox[{"f", "[", "t_", "]"}], ":=", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{
     RowBox[{"Sin", "[", "t", "]"}], "+", 
     RowBox[{"2", " ", 
      RowBox[{"Sin", "[", 
       RowBox[{"2", " ", "t"}], "]"}]}]}], ",", 
    RowBox[{
     RowBox[{"Cos", "[", "t", "]"}], "-", 
     RowBox[{"2", " ", 
      RowBox[{"Cos", "[", 
       RowBox[{"2", " ", "t"}], "]"}]}]}], ",", 
    RowBox[{"-", 
     RowBox[{"Sin", "[", 
      RowBox[{"3", " ", "t"}], "]"}]}]}], "}"}]}]], "Input",
 CellChangeTimes->{{3.9235004123269053`*^9, 3.923500416150341*^9}},
 CellLabel->
  "In[330]:=",ExpressionUUID->"fd230569-e9f6-44aa-96dc-65b614302776"],

Cell[TextData[{
 "where ",
 StyleBox["t", "Input"],
 " can range from ",
 StyleBox["0", "Input"],
 " to ",
 StyleBox["2\[Pi]", "Input"],
 ".  Each point on a curve in R^3 has a corresponding unit tangent vector ",
 StyleBox["T", "Input"],
 ", an unit normal vector ",
 StyleBox["N", "Input"],
 ", and a unit binomial vector ",
 StyleBox["B", "Input"],
 ".  These three vectors are given by the last component of the output of "
}], "Text",
 CellChangeTimes->{{3.923500422591256*^9, 3.9235004314160233`*^9}, {
  3.923508356472775*^9, 
  3.923508356916407*^9}},ExpressionUUID->"6e7fb15e-a167-46b2-9da4-\
37c20f70ba3d"],

Cell[TextData[{
 StyleBox["frame = FrenetSerretSystem[f[t],t]", "Input"],
 "."
}], "Text",
 CellChangeTimes->{{3.9235001121383047`*^9, 3.923500187891518*^9}, {
   3.923500251238851*^9, 3.923500279001419*^9}, {3.923500458411422*^9, 
