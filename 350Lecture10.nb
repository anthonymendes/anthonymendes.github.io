  RowBox[{"f", ",", "g", ",", "u", ",", "k"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"u", "[", 
   RowBox[{"t_", ",", "x_"}], "]"}], ":=", " ", 
  RowBox[{
   RowBox[{
    FractionBox["1", "2"], " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"f", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"-", "k"}], " ", "t"}], "+", "x"}], "]"}], "+", 
      RowBox[{"f", "[", 
       RowBox[{
        RowBox[{"k", " ", "t"}], "+", "x"}], "]"}]}], ")"}]}], "+", 
   RowBox[{
    FractionBox["1", 
     RowBox[{"2", " ", "k"}]], 
    RowBox[{
     SubsuperscriptBox["\[Integral]", 
      RowBox[{
       RowBox[{
        RowBox[{"-", "k"}], " ", "t"}], "+", "x"}], 
      RowBox[{
       RowBox[{"k", " ", "t"}], "+", "x"}]], 
     RowBox[{
      RowBox[{"g", "[", "s", "]"}], 
      RowBox[{"\[DifferentialD]", "s"}]}]}]}]}]}]}], "Input",
