Cell["Plot the vector field in R^3 described by ", "Text",
 CellChangeTimes->{{3.923524230418428*^9, 3.923524234887632*^9}, {
  3.9235243187433233`*^9, 3.923524402016901*^9}, {3.9235244491539097`*^9, 
  3.923524495298847*^9}, {3.923576882931158*^9, 3.9235769873390503`*^9}, {
  3.923579671659678*^9, 3.9235796807255907`*^9}, {3.923579865454279*^9, 
  3.923579867066947*^9}},ExpressionUUID->"339b343f-09a1-4f8e-a3af-\
5e46b37136ef"],

Cell[BoxData[
 RowBox[{
  RowBox[{"F", "[", 
   RowBox[{"x_", ",", "y_", ",", "z_"}], "]"}], ":=", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{"2", "-", "x"}], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            RowBox[{"-", "2"}], "+", "x"}], ")"}], "2"], "+", 
         SuperscriptBox["y", "2"], "+", 
         SuperscriptBox["z", "2"]}], ")"}], 
       RowBox[{"3", "/", "2"}]]], "+", 
     FractionBox[
      RowBox[{"2", "+", "x"}], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"2", "+", "x"}], ")"}], "2"], "+", 
         SuperscriptBox["y", "2"], "+", 
         SuperscriptBox["z", "2"]}], ")"}], 
       RowBox[{"3", "/", "2"}]]]}], ",", 
    RowBox[{"y", " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"-", 
        FractionBox["1", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            SuperscriptBox[
             RowBox[{"(", 
              RowBox[{
               RowBox[{"-", "2"}], "+", "x"}], ")"}], "2"], "+", 
            SuperscriptBox["y", "2"], "+", 
            SuperscriptBox["z", "2"]}], ")"}], 
          RowBox[{"3", "/", "2"}]]]}], "+", 
       FractionBox["1", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"2", "+", "x"}], ")"}], "2"], "+", 
           SuperscriptBox["y", "2"], "+", 
           SuperscriptBox["z", "2"]}], ")"}], 
         RowBox[{"3", "/", "2"}]]]}], ")"}]}], ",", 
    RowBox[{"z", " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"-", 
        FractionBox["1", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            SuperscriptBox[
             RowBox[{"(", 
              RowBox[{
               RowBox[{"-", "2"}], "+", "x"}], ")"}], "2"], "+", 
            SuperscriptBox["y", "2"], "+", 
            SuperscriptBox["z", "2"]}], ")"}], 
          RowBox[{"3", "/", "2"}]]]}], "+", 
       FractionBox["1", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"2", "+", "x"}], ")"}], "2"], "+", 
           SuperscriptBox["y", "2"], "+", 
           SuperscriptBox["z", "2"]}], ")"}], 
         RowBox[{"3", "/", "2"}]]]}], ")"}]}]}], "}"}]}]], "Input",
 CellChangeTimes->{{3.923524250207559*^9, 3.92352426264221*^9}, {
  3.923524298584078*^9, 3.923524303909292*^9}, {3.923524508268482*^9, 
  3.923524508628044*^9}, {3.923579830628525*^9, 3.9235798627341223`*^9}},
 CellLabel->
  "In[534]:=",ExpressionUUID->"b5f0b317-1b03-4217-b7fb-47e383ccf185"],

Cell[TextData[{
 "and then find the work done by this vector field when traveling along the \
curve ",
 StyleBox["{Cos[t],Sin[t],t/(2 \[Pi])}", "Input"],
 " where ",
 StyleBox["t", "Input"],
 " ranges from ",
 StyleBox["0", "Input"],
 " to ",
 StyleBox["2\[Pi]", "Input"],
 " (consider ",
 StyleBox["NIntegrate", "Input"],
 " for the integral)."
}], "Text",
 CellChangeTimes->{{3.923524525634676*^9, 3.923524584043997*^9}, {
   3.923579684643615*^9, 3.923579699479692*^9}, {3.923579873768774*^9, 
