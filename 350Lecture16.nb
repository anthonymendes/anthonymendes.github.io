Cell[TextData[{
 "The fundamental theorem of algebra says that every nonconstant polynomial \
",
 StyleBox["f[x]", "Input"],
 " has a complex root; meaning that there is a complex ",
 StyleBox["c", "Input"],
 " such that ",
 StyleBox["f[c]=0", "Input"],
 ".  Every complex number ",
 StyleBox["c", "Input"],
 " can be expressed in polar form as ",
 StyleBox["c = r E^(i t)", "Input"],
 " where ",
 StyleBox["r", "Input"],
 " is a positive number and ",
 StyleBox["t", "Input"],
 " is an angle between ",
 StyleBox["0", "Input"],
 " and ",
 StyleBox["2 Pi", "Input"],
 ".  Therefore the fundamental theorem of algebra says there is a positive ",
 StyleBox["r", "Input"],
 " and a ",
 StyleBox["t", "Input"],
 " between ",
 StyleBox["0", "Input"],
 " and ",
 StyleBox["2 Pi", "Input"],
 " for which both the real and imaginary parts of ",
 StyleBox["f[r E^(i t)]", "Input"],
 " is equal to ",
 StyleBox["0", "Input"],
 ". \n\nExplain how the below cell can be used to illustrate the validity of \
the fundamental theorem of algebra for the given polynomial ",
 StyleBox["f[x]", "Input"],
 "."
}], "Text",
 CellChangeTimes->{{3.922226201483419*^9, 3.92222628943917*^9}, {
  3.92222633213314*^9, 3.922226355338072*^9}, {3.9222263884820147`*^9, 
  3.92222639857475*^9}, {3.92222695619267*^9, 3.922227133801023*^9}, {
  3.9222296075209837`*^9, 3.922229620899007*^9}, {3.922270818922364*^9, 
  3.922270819732643*^9}, {3.922270861277183*^9, 
  3.922270863780675*^9}},ExpressionUUID->"2a3da17d-37f9-4879-b649-\
a3b1bd2b89c2"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", "x_", "]"}], ":=", 
   RowBox[{
    RowBox[{"x", "^", "3"}], "+", 
    RowBox[{"x", "^", "2"}], "-", "x", "+", "1"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"Manipulate", "[", 
  RowBox[{
   RowBox[{"ParametricPlot", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Re", "[", 
        RowBox[{"f", "[", 
         RowBox[{"r", " ", 
          RowBox[{"E", "^", 
           RowBox[{"(", 
            RowBox[{"I", " ", "t"}], ")"}]}]}], "]"}], "]"}], ",", 
       RowBox[{"Im", "[", 
        RowBox[{"f", "[", 
         RowBox[{"r", " ", 
          RowBox[{"E", "^", 
           RowBox[{"(", 
            RowBox[{"I", " ", "t"}], ")"}]}]}], "]"}], "]"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"t", ",", "0", ",", 
       RowBox[{"2", "Pi"}]}], "}"}], ",", 
     RowBox[{"PlotRange", "->", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"-", "5"}], ",", "5"}], "}"}]}], ",", 
     RowBox[{"AspectRatio", "->", "1"}]}], "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"r", ",", "0", ",", "3"}], "}"}]}], "]"}]}], "Input",
 CellChangeTimes->{{3.92222606243174*^9, 3.922226187467318*^9}, {
