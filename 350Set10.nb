 RowBox[{
  RowBox[{"F", "[", 
   RowBox[{"x_", ",", "y_", ",", "z_"}], "]"}], ":=", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{
     RowBox[{
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{
        RowBox[{"-", "x"}], "-", "y"}]], " ", 
      SuperscriptBox["z", "2"]}], "+", 
     RowBox[{"2", " ", "x", " ", 
      RowBox[{"Cos", "[", 
       RowBox[{"x", " ", "y"}], "]"}]}], "-", 
     RowBox[{
      SuperscriptBox["x", "2"], " ", "y", " ", 
      RowBox[{"Sin", "[", 
       RowBox[{"x", " ", "y"}], "]"}]}]}], ",", 
    RowBox[{
     RowBox[{
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{
        RowBox[{"-", "x"}], "-", "y"}]], " ", 
      SuperscriptBox["z", "2"]}], "-", 
     RowBox[{
      SuperscriptBox["x", "3"], " ", 
      RowBox[{"Sin", "[", 
       RowBox[{"x", " ", "y"}], "]"}]}]}], ",", 
    RowBox[{
     RowBox[{"-", "2"}], " ", 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       RowBox[{"-", "x"}], "-", "y"}]], " ", "z"}]}], "}"}]}]], "Input",Expres\
sionUUID->"0c11162b-7283-4eb5-bf28-716d8de116f9"]
