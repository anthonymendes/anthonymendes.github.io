Cell[TextData[{
 "Find the maximum value of the function ",
 StyleBox["x^(a/x)", "Input"],
 " where both ",
 StyleBox["x", "Input"],
 " and ",
 StyleBox["a", "Input"],
 " are positive."
}], "Text",
 CellChangeTimes->{{3.923420087353355*^9, 
  3.9234201243065233`*^9}},ExpressionUUID->"c0983253-abb0-4571-8108-\
3de9416952b1"]
