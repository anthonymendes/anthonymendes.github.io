 RowBox[{
  RowBox[{"f", "[", "x_", "]"}], ":=", 
  RowBox[{
   RowBox[{"10", " ", 
    RowBox[{"Cos", "[", "x", "]"}]}], "-", "x"}]}]], "Input",
