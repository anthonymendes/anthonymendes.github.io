Cell[BoxData[
 RowBox[{
  RowBox[{"Motzkin", "[", "n_", "]"}], ":=", 
  RowBox[{
   RowBox[{"Motzkin", "[", "n", "]"}], "=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"n", "==", "0"}], ",", 
     RowBox[{"{", 
      RowBox[{"{", "}"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"Join", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"Prepend", "[", 
          RowBox[{"#", ",", "0"}], "]"}], "&"}], "/@", 
        RowBox[{"Motzkin", "[", 
         RowBox[{"n", "-", "1"}], "]"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"Flatten", "[", 
        RowBox[{
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"Flatten", "@", 
            RowBox[{"{", 
             RowBox[{"1", ",", "a", ",", 
              RowBox[{"-", "1"}], ",", "b"}], "}"}]}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "0", ",", 
             RowBox[{"n", "-", "2"}]}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"a", ",", 
             RowBox[{"Motzkin", "[", "i", "]"}]}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"b", ",", 
             RowBox[{"Motzkin", "[", 
              RowBox[{"n", "-", "i", "-", "2"}], "]"}]}], "}"}]}], "]"}], ",",
          "2"}], "]"}]}], "]"}]}], "]"}]}]}]], "Input",
 CellChangeTimes->{{3.9245554321620007`*^9, 3.924555464367284*^9}, {
   3.924555574948997*^9, 3.924555752449993*^9}, {3.924555988387149*^9, 
