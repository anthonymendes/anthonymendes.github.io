  RowBox[{"Graphics", "[", 
   RowBox[{"{", 
    RowBox[{"Red", ",", 
     RowBox[{"Line", "@", 
      RowBox[{"Accumulate", "@", 
       RowBox[{"Prepend", "[", 
        RowBox[{
         RowBox[{"ReplaceAll", "[", 
          RowBox[{"L", ",", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"0", "->", 
              RowBox[{"{", 
               RowBox[{"1", ",", "0"}], "}"}]}], ",", 
             RowBox[{"1", "->", 
              RowBox[{"{", 
               RowBox[{"1", ",", "1"}], "}"}]}], ",", 
             RowBox[{
              RowBox[{"-", "1"}], "->", 
              RowBox[{"{", 
               RowBox[{"1", ",", 
                RowBox[{"-", "1"}]}], "}"}]}]}], "}"}]}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"0", ",", "0"}], "}"}]}], "]"}]}]}]}], "}"}], 
   "]"}]}]], "Input",
