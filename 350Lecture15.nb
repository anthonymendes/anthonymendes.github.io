Cell[BoxData[{
 RowBox[{
  RowBox[{"Compete", "[", 
   RowBox[{"S1_", ",", "S2_", ",", "n_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"H1", "=", 
       RowBox[{"{", "}"}]}], ",", 
      RowBox[{"H2", "=", 
       RowBox[{"{", "}"}]}], ",", 
      RowBox[{"score", "=", 
       RowBox[{"{", 
        RowBox[{"0", ",", "0"}], "}"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{"Module", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"p1", "=", 
            RowBox[{"S1", "@", "H2"}]}], ",", 
           RowBox[{"p2", "=", 
            RowBox[{"S2", "@", "H1"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
         
         RowBox[{
          RowBox[{"AppendTo", "[", 
           RowBox[{"H1", ",", "p1"}], "]"}], ";", "\[IndentingNewLine]", 
          RowBox[{"AppendTo", "[", 
           RowBox[{"H2", ",", "p2"}], "]"}], ";", "\[IndentingNewLine]", 
          RowBox[{"score", "+=", 
           RowBox[{"dilemma", "[", 
            RowBox[{"[", 
             RowBox[{"p1", ",", "p2"}], "]"}], "]"}]}]}]}], "]"}], ",", "n"}],
       "]"}], ";", "\[IndentingNewLine]", "score"}]}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{"Compete", "[", 
  RowBox[{"TitForTat", ",", "WeightedRandom", ",", "1000"}], "]"}]}], "Input",\

