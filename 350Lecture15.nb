Cell[BoxData[{
 RowBox[{
  RowBox[{"Pushover", "[", "H_", "]"}], ":=", "1"}], "\n", 
 RowBox[{
  RowBox[{"TotalJerk", "[", "H_", "]"}], ":=", "2"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"TitForTat", "[", "H_", "]"}], ":=", 
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{"H", "==", 
     RowBox[{"{", "}"}]}], ",", "1", ",", 
    RowBox[{"H", "[", 
     RowBox[{"[", 
      RowBox[{"-", "1"}], "]"}], "]"}]}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ICanForgive", "[", "H_", "]"}], ":=", 
  RowBox[{"Which", "[", 
   RowBox[{
    RowBox[{"H", "==", 
     RowBox[{"{", "}"}]}], ",", "1", ",", 
    RowBox[{
     RowBox[{"H", "[", 
      RowBox[{"[", 
       RowBox[{"-", "1"}], "]"}], "]"}], "==", "2"}], ",", 
    RowBox[{"RandomChoice", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{".1", ",", ".9"}], "}"}], "->", 
      RowBox[{"{", 
       RowBox[{"1", ",", "2"}], "}"}]}], "]"}], ",", "True", ",", "1"}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"WeightedRandom", "[", "H_", "]"}], ":=", 
  RowBox[{"RandomChoice", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{".6", ",", ".4"}], "}"}], "->", 
    RowBox[{"{", 
     RowBox[{"1", ",", "2"}], "}"}]}], "]"}]}], "\n", 
 RowBox[{
  RowBox[{"YouDidThisToMe", "[", "H_", "]"}], ":=", 
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{"H", "==", 
     RowBox[{"{", "}"}]}], ",", "1", ",", 
    RowBox[{"RandomChoice", "@", "H"}]}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"DontMakeMeMad", "[", "H_", "]"}], ":=", 
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"Count", "[", 
      RowBox[{"H", ",", "2"}], "]"}], ">=", "10"}], ",", "2", ",", "1"}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"FalseFront", "[", "H_", "]"}], ":=", 
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"Count", "[", 
      RowBox[{"H", ",", "2"}], "]"}], "<=", "10"}], ",", "2", ",", "1"}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"FoolMeTwice", "[", "H_", "]"}], ":=", 
  RowBox[{"Which", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"Length", "@", "H"}], "<", "2"}], ",", "1", ",", 
    RowBox[{
     RowBox[{"H", "[", 
      RowBox[{"[", 
       RowBox[{
        RowBox[{"-", "2"}], ";;"}], "]"}], "]"}], "==", 
     RowBox[{"{", 
      RowBox[{"2", ",", "2"}], "}"}]}], ",", "2", ",", "True", ",", "1"}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"HalfAndHalf", "[", "H_", "]"}], ":=", 
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{"EvenQ", "@", 
     RowBox[{"Length", "@", "H"}]}], ",", "1", ",", "2"}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"DontMakeMeMadder", "[", "H_", "]"}], ":=", 
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"Count", "[", 
      RowBox[{"H", ",", "2"}], "]"}], ">=", "11"}], ",", "2", ",", "1"}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"AlmostMad", "[", "H_", "]"}], ":=", 
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"Length", "@", "H"}], "<=", "4"}], ",", "2", ",", "1"}], 
   "]"}]}], "\[IndentingNewLine]"}], "Input",
