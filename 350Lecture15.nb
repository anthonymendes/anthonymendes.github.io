           RowBox[{"{", 
            RowBox[{"i", ",", "10"}], "}"}]}], "]"}], ",", "True"}], 
        "]"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"Which", "[", 
     RowBox[{
      RowBox[{"p1win", "<", "p2win"}], ",", 
      RowBox[{"-", "1"}], ",", 
      RowBox[{"p1win", "==", "p2win"}], ",", "0", ",", 
      RowBox[{"p1win", ">", "p2win"}], ",", "1"}], "]"}]}], "]"}]}]], "Input",\

