  3.925391611423958*^9}},ExpressionUUID->"72835b70-c0e1-47ca-b872-\
f403bf673422"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Payoff", "[", 
   RowBox[{"i_", ",", "j_", ",", "r_"}], "]"}], ":=", 
  RowBox[{"Which", "[", 
   RowBox[{
    RowBox[{"i", "==", "j"}], ",", "0", ",", 
    RowBox[{"r", "<", 
     RowBox[{"Min", "[", 
      RowBox[{"{", 
       RowBox[{"i", ",", "j"}], "}"}], "]"}]}], ",", "0", ",", 
    RowBox[{"i", "<=", "r", "<", "j"}], ",", "1", ",", 
    RowBox[{"i", "<", "j", "<=", "r"}], ",", 
    RowBox[{"-", "1"}], ",", 
    RowBox[{"j", "<=", "r", "<", "i"}], ",", 
    RowBox[{"-", "1"}], ",", 
    RowBox[{"j", "<", "i", "<=", "r"}], ",", "1"}], "]"}]}]], "Input",
