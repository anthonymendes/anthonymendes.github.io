   RowBox[{"S1_", ",", "S2_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"p1win", "=", 
       RowBox[{"Count", "[", 
        RowBox[{
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"S1", "[", 
             RowBox[{"[", "i", "]"}], "]"}], ">", 
            RowBox[{"S2", "[", 
             RowBox[{"[", "i", "]"}], "]"}]}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "10"}], "}"}]}], "]"}], ",", "True"}], "]"}]}], 
      ",", 
      RowBox[{"p2win", "=", 
       RowBox[{"Count", "[", 
        RowBox[{
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"S1", "[", 
             RowBox[{"[", "i", "]"}], "]"}], "<", 
            RowBox[{"S2", "[", 
