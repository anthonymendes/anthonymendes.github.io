Cell[TextData[{
 "The Lotka-Volterra equations model a system of prey and predators (like the \
interdependent  populations of rabbits and wolves).  The equations in the \
cell below give one example of a Lotka-Volterra system where ",
 StyleBox["x[t]", "Input"],
 " models the population of prey at time ",
 StyleBox["t", "Input"],
 " and ",
 StyleBox["y[t]", "Input"],
 " is the population of predators at time ",
 StyleBox["t", "Input"],
 ".  "
}], "Text",
 CellChangeTimes->{{3.923760731958363*^9, 3.923760736849387*^9}, {
  3.924001867281546*^9, 3.924001879766906*^9}, {3.924179740945114*^9, 
  3.924179816248104*^9}, {3.924179904341741*^9, 3.9241799154283743`*^9}, {
  3.9241811165958138`*^9, 3.9241811473214006`*^9}, {3.924182900333404*^9, 
  3.924182900688079*^9}},ExpressionUUID->"7f24e794-7909-4957-82c1-\
2af34125eed2"],

Cell[BoxData[
 RowBox[{
  RowBox[{"LVequations", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       SuperscriptBox["x", "\[Prime]",
        MultilineFunction->None], "[", "t", "]"}], "\[Equal]", 
      RowBox[{
       RowBox[{"2", 
        RowBox[{"x", "[", "t", "]"}]}], "-", 
       RowBox[{
        RowBox[{"x", "[", "t", "]"}], " ", 
        RowBox[{"y", "[", "t", "]"}]}]}]}], ",", 
     RowBox[{
      RowBox[{
       SuperscriptBox["y", "\[Prime]",
        MultilineFunction->None], "[", "t", "]"}], "\[Equal]", 
      RowBox[{
       RowBox[{"-", 
        RowBox[{"y", "[", "t", "]"}]}], "+", 
       RowBox[{
        FractionBox["1", "3"], " ", 
        RowBox[{"x", "[", "t", "]"}], " ", 
        RowBox[{"y", "[", "t", "]"}]}]}]}], ",", 
     RowBox[{
      RowBox[{"x", "[", "0", "]"}], "\[Equal]", "2"}], ",", 
     RowBox[{
      RowBox[{"y", "[", "0", "]"}], "\[Equal]", "1"}]}], "}"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.924179819255876*^9, 3.92417986529163*^9}, {
  3.924180432034582*^9, 3.924180438741229*^9}, {3.924180771697081*^9, 
