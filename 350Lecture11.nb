Cell[TextData[{
 "The ",
 StyleBox["LaplaceTransform", "Input"],
 " is an integral transform that can change a differential equation in the \
variable ",
 StyleBox["t", "Input"],
 " into an algebraic equation in the variable ",
 StyleBox["s", "Input"],
 ".  The method to solve a differential equation in is as follows:\n\n1. Take \
the ",
 StyleBox["LaplaceTransform", "Input"],
 " on both sides of the equation.\n2. Solve the new equation for the \
LaplaceTransorm of ",
 StyleBox["y", "Input"],
 ".\n3. Take the ",
 StyleBox["InverseLaplaceTransform", "Input"],
 " to get the final solution.  \n\nUse this method to solve the differential \
equation in the next cell."
}], "Text",
 CellChangeTimes->{{3.9240019749917307`*^9, 3.924001984277607*^9}, {
  3.924203411886735*^9, 3.924203464623934*^9}, {3.924203623085683*^9, 
  3.92420371086423*^9}, {3.924203795316601*^9, 3.924203805021282*^9}, {
  3.9242039605621643`*^9, 
  3.924203961163054*^9}},ExpressionUUID->"016b05b0-9a3e-4f5f-962c-\
812d20139c6c"],

Cell[BoxData[
 RowBox[{
  RowBox[{"equation", "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"y", "'''"}], "[", "t", "]"}], "+", 
     RowBox[{
      RowBox[{"y", "'"}], "[", "t", "]"}]}], "==", 
    RowBox[{
     RowBox[{"-", 
      RowBox[{"Cos", "[", "t", "]"}]}], "+", 
     RowBox[{"1", "/", 
      RowBox[{"(", 
       RowBox[{"1", "+", 
        RowBox[{"t", "^", "2"}]}], ")"}]}]}]}]}], ";"}]], "Input",
